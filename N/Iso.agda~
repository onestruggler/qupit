module Two.Iso where
pattern ‚ÇÄ = zero
pattern ‚ÇÅ = suc ‚ÇÄ
pattern ‚ÇÇ = suc ‚ÇÅ

pattern ‚ÇÅ‚Çä ‚±º = suc ‚±º
pattern ‚ÇÇ‚Çä ‚±º = suc (suc ‚±º)
pattern ‚ÇÉ‚Çä ‚±º = suc (suc (suc ‚±º))

open import Two.Symplectic
open import Two.Symplectic1a
open import Two.Push

module Pauli where
  open OS using (module XZ)
  Gen = OS.XZ.Gen ‚äé OS.XZ.Gen
  
  pattern X‚ÇÄ-gen = inj‚ÇÅ XZ.X-gen
  pattern X‚ÇÅ-gen = inj‚ÇÇ XZ.X-gen
  pattern Z‚ÇÄ-gen = inj‚ÇÅ XZ.Z-gen
  pattern Z‚ÇÅ-gen = inj‚ÇÇ XZ.Z-gen
  
  Z‚ÇÄ : Word Gen
  Z‚ÇÄ = [ Z‚ÇÄ-gen ] ∑
  X‚ÇÄ : Word Gen
  X‚ÇÄ = [ X‚ÇÄ-gen ] ∑

  Z‚ÇÅ : Word Gen
  Z‚ÇÅ = [ Z‚ÇÅ-gen ] ∑
  X‚ÇÅ : Word Gen
  X‚ÇÅ = [ X‚ÇÅ-gen ] ∑

  infix 4 _===_
  _===_ : WRel Gen
  _===_  = (XZ._===_  ‚∏≤ XZ._===_  ‚∏≤ Œì‚Çì)

  pattern order-X‚ÇÄ = left XZ.order-X
  pattern order-Z‚ÇÄ = left XZ.order-Z
  pattern order-X‚ÇÅ = right XZ.order-X
  pattern order-Z‚ÇÅ = right XZ.order-Z
  pattern comm-Z‚ÇÄ-X‚ÇÄ = left XZ.comm-Z-X
  pattern comm-Z‚ÇÅ-X‚ÇÅ = right XZ.comm-Z-X
  pattern comm-Z‚ÇÄ-X‚ÇÅ = mid (comm XZ.Z-gen XZ.X-gen)
  pattern comm-X‚ÇÄ-Z‚ÇÅ = mid (comm XZ.X-gen XZ.Z-gen)
  pattern comm-Z‚ÇÄ-Z‚ÇÅ = mid (comm XZ.Z-gen XZ.Z-gen)
  pattern comm-X‚ÇÄ-X‚ÇÅ = mid (comm XZ.X-gen XZ.X-gen)

  nfp' : NFProperty' _===_
  nfp' = DP.NFP'.nfp' (XZ._===_) (XZ._===_) (XZ.nfp') (XZ.nfp')


module SemiDirect where

  open Symplectic-Lemmas
  open Symplectic-Lemmas3

  open PB (Q‚ÇÄQ‚ÇÅEx._===_) renaming (Alphabet to M ; _===_ to _===‚ÇÄ_ ; _‚âà_ to _‚âà‚ÇÄ_) using ()
  open PP (Q‚ÇÄQ‚ÇÅEx._===_) renaming (word-setoid to ws‚ÇÄ ; by-assoc to bya‚ÇÄ) using ()
  open NFProperty' Q‚ÇÄQ‚ÇÅEx.nfp' using (by-equal-nf)
  open Symplectic-Rewriting2
  open Symplectic-Rewriting
  open Ex-Rewriting

  -- open PB _===_ renaming (Alphabet to MB) using (_‚âà_ ; cleft_ ; cright_ ; sym)
  -- open PP _===_ renaming () using ( by-assoc ; by-assoc-and ; word-setoid)
  -- open SR word-setoid

  open Q‚ÇÄQ‚ÇÅEx-NFP'
  open hH
  open hwd
  open fwd
  open Commuting-Symplectic
  open Symplectic-Powers
  open Clifford-Duality


  open import Presentation.Construct.Base

  Gen = Pauli.Gen ‚äé Symplectic.Gen


  pattern X‚ÇÄ-gen = inj‚ÇÅ Pauli.X‚ÇÄ-gen
  pattern Z‚ÇÄ-gen = inj‚ÇÅ Pauli.Z‚ÇÄ-gen
  pattern H‚ÇÄ-gen = inj‚ÇÇ Symplectic.H‚ÇÄ-gen
  pattern S‚ÇÄ-gen = inj‚ÇÇ Symplectic.S‚ÇÄ-gen
  pattern X‚ÇÅ-gen = inj‚ÇÅ Pauli.X‚ÇÅ-gen
  pattern Z‚ÇÅ-gen = inj‚ÇÅ Pauli.Z‚ÇÅ-gen
  pattern H‚ÇÅ-gen = inj‚ÇÇ Symplectic.H‚ÇÅ-gen
  pattern S‚ÇÅ-gen = inj‚ÇÇ Symplectic.S‚ÇÅ-gen
  pattern CZ-gen = inj‚ÇÇ Symplectic.CZ-gen


  S‚ÇÄ : Word Gen
  S‚ÇÄ = [ S‚ÇÄ-gen ] ∑
  Z‚ÇÄ : Word Gen
  Z‚ÇÄ = [ Z‚ÇÄ-gen ] ∑
  X‚ÇÄ : Word Gen
  X‚ÇÄ = [ X‚ÇÄ-gen ] ∑
  H‚ÇÄ : Word Gen
  H‚ÇÄ = [ H‚ÇÄ-gen ] ∑
  S‚ÇÅ : Word Gen
  S‚ÇÅ = [ S‚ÇÅ-gen ] ∑
  Z‚ÇÅ : Word Gen
  Z‚ÇÅ = [ Z‚ÇÅ-gen ] ∑
  X‚ÇÅ : Word Gen
  X‚ÇÅ = [ X‚ÇÅ-gen ] ∑
  H‚ÇÅ : Word Gen
  H‚ÇÅ = [ H‚ÇÅ-gen ] ∑
  CZ : Word Gen
  CZ = [ CZ-gen ] ∑


  conj : Symplectic.Gen -> Pauli.Gen -> Word Pauli.Gen
  conj Symplectic.H‚ÇÄ-gen Pauli.X‚ÇÄ-gen = Pauli.Z‚ÇÄ
  conj Symplectic.H‚ÇÄ-gen Pauli.Z‚ÇÄ-gen = Pauli.X‚ÇÄ ^ 2
  conj Symplectic.S‚ÇÄ-gen Pauli.X‚ÇÄ-gen = Pauli.X‚ÇÄ ‚Ä¢ Pauli.Z‚ÇÄ
  conj Symplectic.H‚ÇÅ-gen Pauli.X‚ÇÅ-gen = Pauli.Z‚ÇÅ
  conj Symplectic.H‚ÇÅ-gen Pauli.Z‚ÇÅ-gen = Pauli.X‚ÇÅ ^ 2
  conj Symplectic.S‚ÇÅ-gen Pauli.X‚ÇÅ-gen = Pauli.X‚ÇÅ ‚Ä¢ Pauli.Z‚ÇÅ
  conj Symplectic.CZ-gen Pauli.X‚ÇÄ-gen = Pauli.X‚ÇÄ ‚Ä¢ Pauli.Z‚ÇÅ
  conj Symplectic.CZ-gen Pauli.X‚ÇÅ-gen = Pauli.X‚ÇÅ ‚Ä¢ Pauli.Z‚ÇÄ
  conj s w = [ w ] ∑


  pattern order-X‚ÇÄ = left Pauli.order-X‚ÇÄ
  pattern order-Z‚ÇÄ = left Pauli.order-Z‚ÇÄ
  pattern comm-Z‚ÇÄ-X‚ÇÄ = left Pauli.comm-Z‚ÇÄ-X‚ÇÄ
  
  pattern order-X‚ÇÅ = left Pauli.order-X‚ÇÅ
  pattern order-Z‚ÇÅ = left Pauli.order-Z‚ÇÅ
  pattern comm-Z‚ÇÅ-X‚ÇÅ = left Pauli.comm-Z‚ÇÅ-X‚ÇÅ

  pattern comm-Z‚ÇÄ-X‚ÇÅ = left Pauli.comm-Z‚ÇÄ-X‚ÇÅ
  pattern comm-X‚ÇÄ-X‚ÇÅ = left Pauli.comm-X‚ÇÄ-X‚ÇÅ
  pattern comm-Z‚ÇÄ-Z‚ÇÅ = left Pauli.comm-Z‚ÇÄ-Z‚ÇÅ
  pattern comm-X‚ÇÄ-Z‚ÇÅ = left Pauli.comm-X‚ÇÄ-Z‚ÇÅ

  pattern order-H‚ÇÄ = right Symplectic.order-H‚ÇÄ
  pattern order-S‚ÇÄ = right Symplectic.order-S‚ÇÄ
  pattern order-S‚ÇÄH‚ÇÄ = right Symplectic.order-S‚ÇÄH‚ÇÄ
  pattern comm-H‚ÇÄH‚ÇÄS‚ÇÄ = right Symplectic.comm-H‚ÇÄH‚ÇÄS‚ÇÄ
  
  pattern order-H‚ÇÅ = right Symplectic.order-H‚ÇÅ
  pattern order-S‚ÇÅ = right Symplectic.order-S‚ÇÅ
  pattern order-S‚ÇÅH‚ÇÅ = right Symplectic.order-S‚ÇÅH‚ÇÅ
  pattern comm-H‚ÇÅH‚ÇÅS‚ÇÅ = right Symplectic.comm-H‚ÇÅH‚ÇÅS‚ÇÅ

  pattern comm-H‚ÇÅH‚ÇÄ = right Symplectic.comm-H‚ÇÅH‚ÇÄ
  pattern comm-H‚ÇÅS‚ÇÄ = right Symplectic.comm-H‚ÇÅS‚ÇÄ
  pattern comm-S‚ÇÅH‚ÇÄ = right Symplectic.comm-S‚ÇÅH‚ÇÄ
  pattern comm-S‚ÇÅS‚ÇÄ = right Symplectic.comm-S‚ÇÅS‚ÇÄ

  pattern order-CZ = right Symplectic.order-CZ
  pattern comm-CZ-S‚ÇÄ = right Symplectic.comm-CZ-S‚ÇÄ
  pattern comm-CZ-S‚ÇÅ = right Symplectic.comm-CZ-S‚ÇÅ
  pattern comm-CZ-H‚ÇÄH‚ÇÄ = right Symplectic.comm-CZ-H‚ÇÄH‚ÇÄ
  pattern comm-CZ-H‚ÇÅH‚ÇÅ = right Symplectic.comm-CZ-H‚ÇÅH‚ÇÅ

  pattern rel-CZ-H‚ÇÄ-CZ = right Symplectic.rel-CZ-H‚ÇÄ-CZ
  pattern rel-CZ-H‚ÇÅ-CZ = right Symplectic.rel-CZ-H‚ÇÅ-CZ

  pattern conj-H‚ÇÄ-X‚ÇÄ = mid (comm Pauli.X‚ÇÄ-gen Symplectic.H‚ÇÄ-gen)
  pattern conj-H‚ÇÄ-Z‚ÇÄ = mid (comm Pauli.Z‚ÇÄ-gen Symplectic.H‚ÇÄ-gen)
  pattern conj-S‚ÇÄ-X‚ÇÄ = mid (comm Pauli.X‚ÇÄ-gen Symplectic.S‚ÇÄ-gen)
  pattern conj-S‚ÇÄ-Z‚ÇÄ = mid (comm Pauli.Z‚ÇÄ-gen Symplectic.S‚ÇÄ-gen)

  pattern conj-H‚ÇÅ-X‚ÇÅ = mid (comm Pauli.X‚ÇÅ-gen Symplectic.H‚ÇÅ-gen)
  pattern conj-H‚ÇÅ-Z‚ÇÅ = mid (comm Pauli.Z‚ÇÅ-gen Symplectic.H‚ÇÅ-gen)
  pattern conj-S‚ÇÅ-X‚ÇÅ = mid (comm Pauli.X‚ÇÅ-gen Symplectic.S‚ÇÅ-gen)
  pattern conj-S‚ÇÅ-Z‚ÇÅ = mid (comm Pauli.Z‚ÇÅ-gen Symplectic.S‚ÇÅ-gen)

  pattern conj-H‚ÇÅ-X‚ÇÄ = mid (comm Pauli.X‚ÇÄ-gen Symplectic.H‚ÇÅ-gen)
  pattern conj-H‚ÇÅ-Z‚ÇÄ = mid (comm Pauli.Z‚ÇÄ-gen Symplectic.H‚ÇÅ-gen)
  pattern conj-S‚ÇÅ-X‚ÇÄ = mid (comm Pauli.X‚ÇÄ-gen Symplectic.S‚ÇÅ-gen)
  pattern conj-S‚ÇÅ-Z‚ÇÄ = mid (comm Pauli.Z‚ÇÄ-gen Symplectic.S‚ÇÅ-gen)

  pattern conj-H‚ÇÄ-X‚ÇÅ = mid (comm Pauli.X‚ÇÅ-gen Symplectic.H‚ÇÄ-gen)
  pattern conj-H‚ÇÄ-Z‚ÇÅ = mid (comm Pauli.Z‚ÇÅ-gen Symplectic.H‚ÇÄ-gen)
  pattern conj-S‚ÇÄ-X‚ÇÅ = mid (comm Pauli.X‚ÇÅ-gen Symplectic.S‚ÇÄ-gen)
  pattern conj-S‚ÇÄ-Z‚ÇÅ = mid (comm Pauli.Z‚ÇÅ-gen Symplectic.S‚ÇÄ-gen)

  pattern conj-CZ-X‚ÇÅ = mid (comm Pauli.X‚ÇÅ-gen Symplectic.CZ-gen)
  pattern conj-CZ-Z‚ÇÅ = mid (comm Pauli.Z‚ÇÅ-gen Symplectic.CZ-gen)
  pattern conj-CZ-X‚ÇÄ = mid (comm Pauli.X‚ÇÄ-gen Symplectic.CZ-gen)
  pattern conj-CZ-Z‚ÇÄ = mid (comm Pauli.Z‚ÇÄ-gen Symplectic.CZ-gen)


  infix 4 _===_
  _===_ = (Pauli._===_ ‚∏≤ Symplectic._===_ ‚∏≤ Œì‚±º' conj)

  open PB Symplectic._===_ renaming (_===_ to _===‚ÇÇ_ ; _‚âà_ to _‚âà‚ÇÇ_) using ()
  open PP Symplectic._===_ renaming (‚Ä¢-Œµ-monoid to m‚ÇÇ ; word-setoid to ws‚ÇÇ) using ()
  
  open PB Pauli._===_ renaming (_===_ to _===‚ÇÅ_ ; _‚âà_ to _‚âà‚ÇÅ_) using ()
  open PP Pauli._===_ renaming (‚Ä¢-Œµ-monoid to m‚ÇÅ ; word-setoid to ws‚ÇÅ ; by-assoc-and to by-assoc-and‚ÇÅ ; by-assoc to by-assoc‚ÇÅ) using ()

  open PB hiding (_===_)


  open NFProperty' (Pauli.nfp') renaming (by-equal-nf to by-equal-nf‚ÇÅ)

  hyph : ‚àÄ {c d} n -> c ===‚ÇÇ d -> (conj  ∞') c n ‚âà‚ÇÅ (conj  ∞') d n


  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.comm-H‚ÇÅH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.comm-H‚ÇÅS‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.comm-S‚ÇÅH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.comm-S‚ÇÅS‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.order-CZ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.comm-CZ-S‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.comm-CZ-S‚ÇÅ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.comm-CZ-H‚ÇÄH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.comm-CZ-H‚ÇÅH‚ÇÅ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.rel-CZ-H‚ÇÄ-CZ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.rel-CZ-H‚ÇÅ-CZ = by-equal-nf‚ÇÅ auto


  hyph {c} {d} [ Pauli.Z‚ÇÄ-gen ] ∑ Symplectic.comm-H‚ÇÅH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÄ-gen ] ∑ Symplectic.comm-H‚ÇÅS‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÄ-gen ] ∑ Symplectic.comm-S‚ÇÅH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÄ-gen ] ∑ Symplectic.comm-S‚ÇÅS‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÄ-gen ] ∑ Symplectic.order-CZ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÄ-gen ] ∑ Symplectic.comm-CZ-S‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÄ-gen ] ∑ Symplectic.comm-CZ-S‚ÇÅ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÄ-gen ] ∑ Symplectic.comm-CZ-H‚ÇÄH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÄ-gen ] ∑ Symplectic.comm-CZ-H‚ÇÅH‚ÇÅ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÄ-gen ] ∑ Symplectic.rel-CZ-H‚ÇÄ-CZ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÄ-gen ] ∑ Symplectic.rel-CZ-H‚ÇÅ-CZ = by-equal-nf‚ÇÅ auto


  hyph {c} {d} [ Pauli.X‚ÇÅ-gen ] ∑ Symplectic.comm-H‚ÇÅH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÅ-gen ] ∑ Symplectic.comm-H‚ÇÅS‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÅ-gen ] ∑ Symplectic.comm-S‚ÇÅH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÅ-gen ] ∑ Symplectic.comm-S‚ÇÅS‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÅ-gen ] ∑ Symplectic.order-CZ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÅ-gen ] ∑ Symplectic.comm-CZ-S‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÅ-gen ] ∑ Symplectic.comm-CZ-S‚ÇÅ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÅ-gen ] ∑ Symplectic.comm-CZ-H‚ÇÄH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÅ-gen ] ∑ Symplectic.comm-CZ-H‚ÇÅH‚ÇÅ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÅ-gen ] ∑ Symplectic.rel-CZ-H‚ÇÄ-CZ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÅ-gen ] ∑ Symplectic.rel-CZ-H‚ÇÅ-CZ = by-equal-nf‚ÇÅ auto


  hyph {c} {d} [ Pauli.Z‚ÇÅ-gen ] ∑ Symplectic.comm-H‚ÇÅH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÅ-gen ] ∑ Symplectic.comm-H‚ÇÅS‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÅ-gen ] ∑ Symplectic.comm-S‚ÇÅH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÅ-gen ] ∑ Symplectic.comm-S‚ÇÅS‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÅ-gen ] ∑ Symplectic.order-CZ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÅ-gen ] ∑ Symplectic.comm-CZ-S‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÅ-gen ] ∑ Symplectic.comm-CZ-S‚ÇÅ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÅ-gen ] ∑ Symplectic.comm-CZ-H‚ÇÄH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÅ-gen ] ∑ Symplectic.comm-CZ-H‚ÇÅH‚ÇÅ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÅ-gen ] ∑ Symplectic.rel-CZ-H‚ÇÄ-CZ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.Z‚ÇÅ-gen ] ∑ Symplectic.rel-CZ-H‚ÇÅ-CZ = by-equal-nf‚ÇÅ auto

  hyph {c} {d} Œµ Symplectic.comm-H‚ÇÅH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} Œµ Symplectic.comm-H‚ÇÅS‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} Œµ Symplectic.comm-S‚ÇÅH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} Œµ Symplectic.comm-S‚ÇÅS‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} Œµ Symplectic.order-CZ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} Œµ Symplectic.comm-CZ-S‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} Œµ Symplectic.comm-CZ-S‚ÇÅ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} Œµ Symplectic.comm-CZ-H‚ÇÄH‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} Œµ Symplectic.comm-CZ-H‚ÇÅH‚ÇÅ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} Œµ Symplectic.rel-CZ-H‚ÇÄ-CZ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} Œµ Symplectic.rel-CZ-H‚ÇÅ-CZ = by-equal-nf‚ÇÅ auto

  hyph {c} {d} (n ‚Ä¢ n‚ÇÅ) eq@ Symplectic.comm-H‚ÇÅH‚ÇÄ = cong (hyph n eq) (hyph n‚ÇÅ eq) 
  hyph {c} {d} (n ‚Ä¢ n‚ÇÅ) eq@ Symplectic.comm-H‚ÇÅS‚ÇÄ = cong (hyph n eq) (hyph n‚ÇÅ eq) 
  hyph {c} {d} (n ‚Ä¢ n‚ÇÅ) eq@ Symplectic.comm-S‚ÇÅH‚ÇÄ = cong (hyph n eq) (hyph n‚ÇÅ eq) 
  hyph {c} {d} (n ‚Ä¢ n‚ÇÅ) eq@ Symplectic.comm-S‚ÇÅS‚ÇÄ = cong (hyph n eq) (hyph n‚ÇÅ eq) 
  hyph {c} {d} (n ‚Ä¢ n‚ÇÅ) eq@ Symplectic.order-CZ = cong (hyph n eq) (hyph n‚ÇÅ eq) 
  hyph {c} {d} (n ‚Ä¢ n‚ÇÅ) eq@ Symplectic.comm-CZ-S‚ÇÄ = cong (hyph n eq) (hyph n‚ÇÅ eq) 
  hyph {c} {d} (n ‚Ä¢ n‚ÇÅ) eq@ Symplectic.comm-CZ-S‚ÇÅ = cong (hyph n eq) (hyph n‚ÇÅ eq) 
  hyph {c} {d} (n ‚Ä¢ n‚ÇÅ) eq@ Symplectic.comm-CZ-H‚ÇÄH‚ÇÄ = cong (hyph n eq) (hyph n‚ÇÅ eq) 
  hyph {c} {d} (n ‚Ä¢ n‚ÇÅ) eq@ Symplectic.comm-CZ-H‚ÇÅH‚ÇÅ = cong (hyph n eq) (hyph n‚ÇÅ eq) 
  hyph {c} {d} (n ‚Ä¢ n‚ÇÅ) eq@ Symplectic.rel-CZ-H‚ÇÄ-CZ = cong (hyph n eq) (hyph n‚ÇÅ eq) 
  hyph {c} {d} (n ‚Ä¢ n‚ÇÅ) eq@ Symplectic.rel-CZ-H‚ÇÅ-CZ = cong (hyph n eq) (hyph n‚ÇÅ eq)


  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.order-H‚ÇÄ = by-equal-nf‚ÇÅ auto
  hyph {c} {d} [ Pauli.X‚ÇÄ-gen ] ∑ Symplectic.order-S‚ÇÄ = by-equal-nf‚ÇÅ auto
