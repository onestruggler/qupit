lemma-Ex-S^ᵏ : ∀ k → let open PB ((₂₊ n) QRel,_===_) in 

  Ex • S^ k ≈ S^ k ↑ • Ex

lemma-Ex-S^ᵏ k = begin
  Ex • S^ k ≈⟨ lemma-Ex-Sᵏ (toℕ k) ⟩
  S ↑ ^ toℕ k • Ex ≈⟨ (cleft refl' (aux-↑ S (toℕ k))) ⟩
  S^ k ↑ • Ex ∎
  where
  open PB ((₂₊ n) QRel,_===_)
  open PP ((₂₊ n) QRel,_===_)
  open SR word-setoid




open import Algebra.Properties.Ring (+-*-ring p-2)
open PB ((₂₊ n) QRel,_===_)
open PP ((₂₊ n) QRel,_===_)
